<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings version="2.1" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
               xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations" xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
               xmlns:inheritance="http://jaxb2-commons.dev.java.net/basic/inheritance" xmlns:annox="http://annox.dev.java.net"
               jaxb:extensionBindingPrefixes="hj orm inheritance" schemaLocation="../xsd/1.0/IR_MonitoringStation_v1.0.xsd">

    <jaxb:bindings node="//xsd:complexType[@name='MonitoringStationDataType']">
        <jaxb:bindings node="//xsd:element[@ref='IR:MonitoringDataLinkText']">
            <hj:basic>
                <orm:column name="MON_DATA_LINK_TXT"/>
            </hj:basic>
        </jaxb:bindings>
    </jaxb:bindings>
</jaxb:bindings>
